#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/abel/Documents/study/架构/tianbenchu-cli/node_modules/.pnpm/pure-thin-cli@0.1.8/node_modules/pure-thin-cli/bin/node_modules:/Users/abel/Documents/study/架构/tianbenchu-cli/node_modules/.pnpm/pure-thin-cli@0.1.8/node_modules/pure-thin-cli/node_modules:/Users/abel/Documents/study/架构/tianbenchu-cli/node_modules/.pnpm/pure-thin-cli@0.1.8/node_modules:/Users/abel/Documents/study/架构/tianbenchu-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/abel/Documents/study/架构/tianbenchu-cli/node_modules/.pnpm/pure-thin-cli@0.1.8/node_modules/pure-thin-cli/bin/node_modules:/Users/abel/Documents/study/架构/tianbenchu-cli/node_modules/.pnpm/pure-thin-cli@0.1.8/node_modules/pure-thin-cli/node_modules:/Users/abel/Documents/study/架构/tianbenchu-cli/node_modules/.pnpm/pure-thin-cli@0.1.8/node_modules:/Users/abel/Documents/study/架构/tianbenchu-cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/www.js" "$@"
else
  exec node  "$basedir/../../bin/www.js" "$@"
fi
